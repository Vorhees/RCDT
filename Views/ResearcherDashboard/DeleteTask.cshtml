@model RCDT.Models.TaskModel

@{
    ViewData["Title"] = "Delete Task";
}
<div class="container mt-2">
    <div style="margin-top:50px">
      <div class="text-center" style="margin-top:200px">
        <h1 class="display-4">Delete Task: @Model.TaskSessionID ?</h1>
            <form asp-action="DeleteTask">
                <input type="submit" value="Delete" class="btn btn-danger" />
                <button class="btn btn-primary" type="button" onclick="location.href='@Url.Action("Index" , "ResearcherDashboard")'"> Cancel
                </button>
            </form>
     </div>
    </div>
</div>
